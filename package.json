{
  "name": "trackaudio",
  "productName": "TrackAudio",
  "version": "1.4.0-beta.1",
  "main": "./out/main/index.js",
  "author": "github.com/pierr3",
  "description": "TrackAudio - Audio for VATSIM Client",
  "homepage": "https://github.com/pierr3/TrackAudio",
  "engines": {
    "node": ">=22.0.0 <=23.0.0",
    "electron": ">=40.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "lint": "eslint . -c eslint.config.mjs --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir -c electron-builder-config.js",
    "build:win": "pnpm run build && electron-builder --win -c electron-builder-config.js",
    "build:mac": "pnpm run build && electron-builder --mac -c electron-builder-config.js",
    "build:linux": "pnpm run build && electron-builder --linux -c electron-builder-config.js",
    "build:backend": "cd backend && pnpm install && pnpm run build:release && cd .. && pnpm install ./backend/trackaudio-afv-1.0.0.tgz",
    "build:backend-fast": "cd backend && pnpm install && pnpm run build:release-fast && cd .. && pnpm install ./backend/trackaudio-afv-1.0.0.tgz",
    "build:backend-debug": "cd backend && pnpm install && pnpm run build:debug && cd .. && pnpm install ./backend/trackaudio-afv-1.0.0.tgz"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "bootstrap": "^5.3.8",
    "bootstrap-scss": "^5.3.8",
    "bootstrap-typeahead": "^2.3.2",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.7.3",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.3.1",
    "react-responsive": "^10.0.1",
    "scss": "^0.2.4",
    "trackaudio-afv": "file:backend/trackaudio-afv-1.0.0.tgz",
    "use-debounce": "^10.1.0",
    "zod": "^3.24.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "2.1.0",
    "@electron-toolkit/eslint-config-ts": "3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/node": "^25.2.3",
    "@types/react": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.4",
    "electron": "^40.4.0",
    "electron-builder": "26.6.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  }
}
